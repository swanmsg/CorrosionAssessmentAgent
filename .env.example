# 阿里百炼API配置
DASHSCOPE_API_KEY=your_dashscope_api_key_here
QWEN_MODEL=qwen-plus

# 可选的OpenAI配置（向后兼容）
# OPENAI_API_KEY=your_openai_api_key_here
# OPENAI_MODEL=gpt-4

# 模型配置
CORROSION_MODEL_PATH=data/models/corrosion_detector.pth
IMAGE_MODEL_PATH=data/models/image_classifier.pth

# 数据路径
DATA_ROOT_PATH=data/
SAMPLE_DATA_PATH=data/sample/
OUTPUT_PATH=outputs/

# 日志配置
LOG_LEVEL=INFO
LOG_FILE=logs/corrosion_agent.log

# Agent配置
MAX_ITERATIONS=10
TIMEOUT_SECONDS=300

# 传感器配置
SENSOR_POLLING_INTERVAL=60
IMAGE_CAPTURE_INTERVAL=300

# 风险阈值
LOW_RISK_THRESHOLD=0.3
MEDIUM_RISK_THRESHOLD=0.6
HIGH_RISK_THRESHOLD=0.8